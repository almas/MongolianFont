feature calt { 
  script mong; 
  lookup calt1 { 
  lookupflag IgnoreMarks;
    sub ML_UE.init' Й И by титэм гэдэс.init1;
    sub @initials ML_UE.medi' Й И by гэдэс.medi;
    sub @initials [ML_OE ML_UE]' by гэдэс шилбэ.medi1;
    sub ML_NA.medi' @vowel by цэгтэй шүд.medi1;
    sub ML_NA.medi' @consonant by шүд.medi;
    sub ML_YA.medi' [ML_A ML_E ML_O ML_U Ү] дараа [И] by шилбэ.medi;
    sub ML_YA.medi' @consonant by шилбэ.medi;
    sub ML_YA.medi' @vowel by ээтгэр шилбэ.medi1;
    sub ML_DA.medi' @vowel by гогцоотой шилбэ.medi1;
    sub ML_DA.medi' @consonant by гэдэс шүд.medi;
    sub ML_GA.init' [ML_A ML_O ML_U] by нум шүд хоёр цэг.init;
    sub ML_GA.medi' [ML_A ML_O ML_U] by хоёр шүд хоёр цэгтэй.medi1;
    sub [ML_A ML_O ML_U] ML_GA.medi' @consonant by хоёр шүд.medi;
    sub [ML_A ML_O ML_U] ML_GA.fina' by шүд ахар сүүл.fina;
    sub [ML_E ML_OE ML_UE ML_EE] ML_GA.fina' by шилбэ орхиц.fina1;
    sub [ML_A ML_O ML_U] И ML_GA.fina' by шүд ахар сүүл.fina;
    sub [ML_A ML_O ML_U] [ямар нэг үсэг] И ML_GA.fina' by шүд ахар сүүл.fina;
    sub [ML_E ML_OE ML_UE] И ML_GA.fina' by шүд ахар сүүл.fina;
    sub [ML_E ML_OE ML_UE] [ямар нэг үсэг] И ML_GA.fina' by шүд ахар сүүл.fina;
    sub И @consonant И ML_GA.fina' by шүд ахар сүүл.fina;
    sub ML_QA.init' [ML_A ML_O ML_U] by нум шүд.init;
    sub ML_QA.medi' [ML_A ML_O ML_U] by хоёр шүд.medi;
    sub ML_JA.init' MVS by нум.medi1;
    sub [Н Х Г Ж Й В].medi' MVS [ML_A ML_E] by цэгтэй сүүл=medi2, шүд ахар сүүл=medi3, шүд ахар сүүл хоёр цэгтэй=medi2, нум=medi1, нум=medi2, гэдэс нум.fina1;
    sub ML_NA.medi' FSV1 MVS [ML_A ML_E] by сүүл.fina;
    sub [ML_A ML_E] MVS.fina' by орхиц.fina1;
    sub NNBSP ML_A' Ч by титэм.medi2;
    sub NNBSP И ML_YA' by шилбэ.medi;
    sub NNBSP ML_YA' И by шилбэ.medi;
    sub NNBSP [ML_DA ML_TA ML_LA ML_NA ML_YA] ML_UE' by гэдэс.medi;
    sub NNBSP [У Ү]' [space NNBS] by GEDES_NUM.fina;
    sub NNBSP [У Ү]' [ML_NA ML_DA] by гэдэс.medi;
    sub NNBSP ML_DA' by гогцоотой шилбэ.medi1;
    sub NNBSP ML_I' [space NNBS] by нум.fina;
  } calt1;
language MNG ;
} calt;