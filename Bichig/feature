feature isol { 
  script mong; 
  lookup isol1 { 
  lookupflag IgnoreMarks;
    sub ML_A FVS1 by TITEM_ORHITS.isol1;
    sub ML_UE FVS1 by титэм гэдэс нум.isol1;
    sub ML_QA FVS1 by нум хоёр цэгтэй.isol1;
    sub ML_A by TITEM_ORHITS.isol;
    sub ML_E by TITEM_ORHITS.isol;
    sub ML_I by титэм нум.isol;
    sub ML_O by титэм гэдэс.isol;
    sub ML_U by титэм гэдэс.isol;
    sub ML_OE by титэм гэдэс оготор сүүл.isol;
    sub ML_UE by титэм гэдэс оготор сүүл.isol;
    sub ML_EE by титэм мойтгор шилбэ оготор сүүл.isol;
    sub ML_QA by нум богино нуруутай.isol;
    sub ML_GA by нум богино нуруутай.isol;
    sub ML_ZHI by ML-ZHI;
    sub ML_CHI by ML-CHI;
  } isol1;
language MNG ;
} isol;
feature fina { 
  script mong; 
  lookup fina1 { 
  lookupflag IgnoreMarks;
    sub ML_A FVS1 by ML_A.fina1;
    sub ML_E FVS1 by ML_E.fina1;
    sub ML_O FVS1 by ML_O.fina1;
    sub ML_OE FVS1 by ML_OE.fina1;
    sub ML_UE FVS1 by ML_UE.fina1;
    sub ML_BA FVS1 by ML_BA.fina1;
    sub ML_GA FVS1 by ML_GA.finafvs1;
    sub ML_SA FVS1 by ML_SA.fina1;
    ;
    ;
    sub ML_SA FVS2 by ML_SA.fina2;
    sub ML_A by сүүл.fina;
    sub ML_E by сүүл.fina;
    sub ML_I by нум.fina;
    sub [ML_O ML_U] by GEDES_NUM.fina;
    sub [ML_OE ML_UE] by GEDES_NUM.fina;
    sub ML_EE by мойтгор шилбэ оготор сүүл.fina;
    sub ML_NA by сүүл.fina;
    sub ML_ANG by шүд шилбэ орхиц.fina;
    sub ML_BA by гэдэс нум орхиц.fina;
    sub ML_PA by жавж гэдэс нум орхиц.fina;
    sub ML_QA by нум.medi;
    sub ML_GA by хоёр шүд ахар сүүл.fina;
    sub ML_MA by ахар сүүл гэзэг.fina;
    sub ML_LA by ахар сүүл эвэр.fina;
    sub ML_SA by завж ахар сүүл.fina;
    sub ML_SHA by завж ахар сүүл ардаа хоёр цэг.fina;
    sub ML_TA by босоо Д оготор сүүл.fina;
    sub ML_DA by гэдэс сүүл.fina;
    sub ML_CHA by өнцөгт эвэр оготор сүүл.fina;
    sub ML_JA by мохоо эвэр оготор сүүл.fina;
    sub ML_YA by нум.fina;
    sub ML_RA by зөрүү шилбэ нум.fina;
    sub ML_WA by мойтгор шилбэ оготор сүүл.fina;
    sub ML_FA by эвэртэй гэдэс нум орхиц.fina;
    sub ML_KA by ML_KA.fina;
    sub ML_KHA by ML_KHA.fina;
    sub ML_TSA by цагирагтай өгцөгт эвэр оготор сүүл.fina;
    sub ML_ZA by зартигтай өнцөгт эвэр оготор сүүл.fina;
    sub ML_HAA by ML_HAA.fina;
    sub ML_ZRA by ML_ZRA.fina;
    sub ML_ZHI by ML-ZHI;
    sub ML_CHI by ML-CHI;
  } fina1;
language MNG ;
} fina;
feature init { 
  script mong; 
  lookup init1 { 
  lookupflag IgnoreMarks;
    sub ML_E FVS1 by ML_E.init1;
    sub ML_NA FVS1 by ML_NA.init1;
    sub ML_QA FVS1 by ML_QA.init1;
    sub ML_GA FVS1 by ML_GA.init1;
    sub ML_DA FVS1 by ML_DA.init1;
    sub ML_YA FVS1 by ML_YA.init1;
    sub ML_A by титэм шүд.init;
    sub ML_E by титэм.init;
    sub ML_I by титэм шилбэ.init;
    sub [ML_O ML_U] by титэм гэдэс.init;
    sub [ML_OE ML_UE] by титэм гэдэс шилбэ.init;
    sub ML_EE by титэм мойтгор шилбэ.init;
    sub ML_NA by титэм цэгтэй.init;
    sub ML_QA by титэм хагархай гэдэстэй шүд.init;
    sub ML_GA by титэм хагархай гэдэстэй шүд хоёр цэгтэй.init;
    sub ML_MA by титэм гэзэг.init;
    sub ML_LA by титэм эвэр.init;
    sub ML_SA by ML_SA.init;
    sub ML_SHA by ML_SHA.init;
    sub ML_TA by ML_TA.init;
    sub ML_DA by ML_DA.init;
    sub ML_CHA by ML_CHA.init;
    sub ML_JA by ML_JA.init;
    sub ML_YA by ML_YA.init;
    sub ML_RA by ML_RA.init;
    sub ML_WA by мойтгор шилбэ.init;
    sub ML_TSA by ML_TSA.init;
    sub ML_ZA by ML_ZA.init;
    sub ML_HAA by ML_HAA.init;
    sub ML_ZRA by ML_ZRA.init;
    sub ML_LHA by ML_LHA.init;
    sub ML_ZHI by ML-ZHI;
    sub ML_CHI by ML-CHI;
  } init1;
language MNG ;
} init;
feature medi { 
  script mong; 
  lookup medi1 { 
  lookupflag IgnoreMarks;
    sub ML_A FVS1 by ML_A.medi1;
    sub ML_I FVS1 by ML_I.medi1;
    sub ML_O FVS1 by ML_O.medi1;
    sub ML_U FVS1 by ML_U.medi1;
    sub ML_OE FVS1 by ML_OE.medi1;
    sub ML_UE FVS1 by ML_UE.medi1;
    sub ML_NA FVS1 by ML_NA.medifvs1;
    sub ML_QA FVS1 by ML_QA.medi1;
    sub ML_GA FVS1 by ML_GA.medifvs1;
    sub ML_TA FVS1 by ML_TA.medi1;
    sub ML_DA FVS1 by ML_DA.medi1;
    sub ML_JA FVS1 by ML_JA.medi1;
    sub ML_YA FVS1 by ML_YA.medifvs1;
    sub ML_WA FVS1 by ML_WA.medi1;
    sub ML_A FVS2 by ML_A.medi2;
    sub ML_OE FVS2 by ML_OE.medi2;
    sub ML_UE FVS2 by ML_UE.medi2;
    sub ML_NA FVS2 by ML_NA.medi2;
    sub ML_QA FVS2 by ML_QA.medi2;
    sub ML_GA FVS2 by ML_GA.medi2;
    sub ML_YA FVS2 by ML_YA.medi2;
    sub ML_NA FVS3 by ML_NA.medi3;
    sub ML_QA FVS3 by ML_QA.medi3;
    sub ML_GA FVS3 by ML_GA.medifvs3;
    sub ML_A by шүд.medi;
    sub ML_E by шүд.medi;
    sub ML_I by шилбэ.medi;
    sub [ML_O ML_U] by гэдэс.medi;
    sub [ML_OE ML_UE] by гэдэс.medi;
    sub ML_EE by мойтгор шилбэ.medi;
    sub ML_NA by цэгтэй шүд.medi1;
    sub ML_ANG by шүд нум.medi;
    sub ML_QA by хоёр шүд.medi;
    sub ML_GA by хоёр шүд хоёр цэгтэй.medi1;
    sub ML_MA by шүд гэзэг.medi;
    sub ML_LA by шүд эвэр.medi;
    sub ML_SA by ML_SA.medi;
    sub ML_SHA by ML_SHA.medi;
    sub ML_TA by ML_TA.medi;
    sub ML_DA by ML_DA.medi1;
    sub ML_CHA by ML_CHA.medi;
    sub ML_JA by ML_JA.medi;
    sub ML_YA by ээтгэр шилбэ.medi1;
    sub ML_RA by ML_RA.medi;
    sub ML_WA by мойтгор шилбэ.medi;
    sub ML_TSA by ML_TSA.medi;
    sub ML_ZA by ML_ZA.medi;
    sub ML_HAA by ML_HAA.medi;
    sub ML_ZRA by ML_ZRA.medi;
    sub ML_LHA by ML_LHA.medi;
    sub ML_ZHI by ML-ZHI;
    sub ML_CHI by ML-CHI;
    ;
    sub ML_UE үгийн эхэнд [Й][И] by ML_UE.титэм гэдэс=init1;
    sub ML_UE үгийн эхний үед [Й][И] by ML_UE.гэдэс=medi;
    ;
    sub ML_NA үгийн дунд [эгшиг] by ML_NA.цэгтэй шүд=medi1;
    sub ML_NA үгийн дунд [гийгүүлэгч] by ML_NA.шүд=medi;
    sub ML_YA үгийн дунд [А Э О У Ү] дараа [И] by ML_YA.шилбэ=medi;
    sub ML_YA үгийн дунд [гийгүүлэгч] by ML_YA.шилбэ=medi;
    sub ML_YA үгийн дунд [эгшиг] by ML_YA.ээтгэр шилбэ=medi1;
    sub ML_DA үгийн дунд [эгшиг] by ML_DA.гогцоотой шилбэ=medi1;
    sub ML_DA үгийн дунд [гийгүүлэгч] by ML_DA.гэдэс шүд=medi;
    sub ML_GA үгийн эхэнд [А О У] by ML_GA.нум шүд хоёр цэг=init;
    sub ML_GA үгийн дунд [А О У] by ML_GA.хоёр шүд хоёр цэгтэй=medi1;
    ;
    ;
    ;
    ;
    ;
    ;
    ;
    ;
    sub ML_QA үгийн эхэнд [А О У] by ML_QA.нум шүд=init;
    sub ML_QA үгийн дунд [А О У] by ML_QA.хоёр шүд=medi;
    sub ML_JA үгийн эхэнд [MVS] by ML_JA.нум=medi1;
    sub [Н Х Г Ж Й В] үгийн дунд [MVS][А Э] by [Н Х Г Ж Й В].цэгтэй сүүл=medi2, шүд ахар сүүл=medi3, шүд ахар сүүл хоёр цэгтэй=medi2, нум=medi1, нум=medi2, гэдэс нум=fina1;
    sub ML_NA үгийн дунд [FVS1][MVS][А Э] by ML_NA.сүүл=fina;
    sub [А Э] үгийн адагт [MVS] by [А Э].орхиц=fina1;
    sub ML_A нөхцөлд [NNBSP] дараа [Ч] by ML_A.титэм=medi2;
    ;
    sub ML_YA нөхцөлд [NNBSP] дараа [И] by ML_YA.шилбэ=medi;
    ;
    sub [У Ү] нөхцөлд [NNBSP] дараа [space NNBS] by [У Ү].гэдэс нум=fina;
    sub [У Ү] нөхцөлд [NNBSP] дараа [Н Д] by [У Ү].гэдэс=medi;
    ;
    sub ML_I нөхцөлд [NNBSP] дараа [space NNBS] by ML_I.нум=fina;
    ;
    ;
    ;
    ;
    ;
  } medi1;
language MNG ;
} medi;